{
  "name": "geocloud2-dev",
  "dockerComposeFile": [
    "../docker/docker-compose.yml"
  ],
  "service": "gc2core",
  "runServices": [
    "postgis",
    "redis"
  ],
  "workspaceFolder": "../",
  "overrideCommand": false,
  "remoteUser": "root",
  "shutdownAction": "stopCompose",
  "postCreateCommand": "true",
  "forwardPorts": [
    8080,
    8081,
    8088,
    5432,
    6379
  ],
  "features": {},
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-azuretools.vscode-docker",
        "ms-vscode.vscode-typescript-next",
        "xdebug.php-pack",
        "esbenp.prettier-vscode"
      ]
    }
  }
}
